##### 脚本变量 ##### scripts\build.sh
export PROGRAMFILES="D:\Program Files"
export WINDOWS_KITS_PATH="D:\Windows Kits"
# 控制是否跳过上传步骤。（1跳过上传，0执行上传操作）
export OSGEO4W_SKIP_UPLOAD=1
# 控制是否跳过清理步骤。(1跳过清理，0执行源码构建目录清理)
export OSGEO4W_SKIP_CLEAN=0
# 控制是否构建反向依赖，依赖当前包的其他包。（1构建反向依赖，0仅构建指定的包）
export OSGEO4W_BUILD_RDEPS=0
# 控制构建失败时是否继续后续包的构建。(0失败即停止，1继续构建其他包)
export OSGEO4W_CONTINUE_BUILD=0
# 控制是否跳过从主仓库下载依赖。(0从主仓库下载依赖，1不会从主仓库下载依赖)
export OSGEO4W_SKIP_MASTER_REPO=0
