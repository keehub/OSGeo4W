# 跳过源码清理步骤（git clean、git reset 等），用于保留本地修改
export OSGEO4W_SKIP_CLEAN=1
# 跳过测试步骤，仅执行构建和打包
export OSGEO4W_SKIP_TESTS=1
# 强制重新构建，即使当前 commit 已构建过（覆盖版本检查逻辑）
export OSGEO4W_FORCE_REBUILD=1
# 控制是否将构建 / 测试结果提交到 CDash 平台
export PUSH_TO_DASH=FALSE
# 跳过安装步骤，不生成安装目录和安装包	
# export OSGEO4W_SKIP_INSTALL=1
# 源码目录路径，指向 QGIS 源码所在位置
# export SRCDIR=1
export REPO="https://github.com/keehub/QGIS.git"
# QGIS源码目录
export QGIS_SOURCE_DIR="C:\ProgramHub\QGIS"
